{
  "manifest_version": 2,
  "name": "Tab Snooze",
  "version": "0.0.1",
  "description": "Snooze tabs you want to get back to later",
  "icons": {
    "128": "/icons/snooze.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["alarms", "tabs", "storage"]
}
